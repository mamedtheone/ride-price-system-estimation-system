   "source": [
    "## 3. Data Cleaning & Feature Engineering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = df.drop_duplicates()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['traffic'] = df['traffic'].str.lower().str.strip().replace({'low_': 'low'})\n",
    "df['weather'] = df['weather'].str.lower().str.strip()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Remove negative duration values\n",
    "df = df[df['duration'] > 0]\n",
    "\n",
    "# Cap extreme values instead of deleting\n",
    "df['distance'] = df['distance'].clip(upper=100)\n",
    "df['price'] = df['price'].clip(upper=300)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_encoded = pd.get_dummies(\n",
    "    df,\n",
    "    columns=['traffic', 'weather', 'time_of_day', 'demand'],\n",
    "    drop_first=True\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "scaler = StandardScaler()\n",
    "df_encoded[['distance', 'duration']] = scaler.fit_transform(\n",
    "    df_encoded[['distance', 'duration']]\n",
    ")"
   ]
  },
